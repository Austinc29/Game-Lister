Title: <%= @game.title%><br>
Release Date: <%= @game&.release_date&.strftime('%B %e, %Y') %><br>
Description: <%= @game.description %><br>

<form method="post" action="/games/<%= @game.id %>">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="delete">
</form>